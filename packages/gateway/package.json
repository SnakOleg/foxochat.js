{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@foxogram/gateway",
  "version": "0.1.5",
  "description": "The Gateway client for foxogram.js",
  "author": "FoxoCorp",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/foxocorp/foxogram.js"
  },
  "bugs": {
    "url": "https://github.com/foxocorp/foxogram.js/issues"
  },
  "scripts": {
    "lint": "eslint",
    "build": "rslib build",
    "format": "prettier --write .",
    "prepack": "pnpm run build"
  },
  "prettier": {
    "printWidth": 120
  },
  "dependencies": {
    "@foxogram/api-types": "workspace:^",
    "@foxogram/gateway-types": "workspace:^",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@rslib/core": "^0.6.8",
    "@tsconfig/strictest": "^2.0.5",
    "eslint": "^9.26.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
