{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@foxocorp/foxogram.js",
  "version": "0.0.0",
  "description": "A powerful library for interacting with the Foxogram API",
  "author": "FoxoCorp",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
